{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "paths": {
      "~/*": ["src/*"]
    },
    "types": ["@cloudflare/workers-types"]
  },
  "include": ["src/**/*", "functions/**/*"],
  "exclude": ["node_modules"]
}
